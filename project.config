[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Amit Talwar (intelliriffer)
[access "refs/for/refs/*"]
	read = group user/Amit Talwar (intelliriffer)
	push = group user/Amit Talwar (intelliriffer)
	label-Code-Review = -2..+2 group user/Amit Talwar (intelliriffer)
	label-Verified = -1..+1 group user/Amit Talwar (intelliriffer)
	rebase = group user/Amit Talwar (intelliriffer)
	removeReviewer = group user/Amit Talwar (intelliriffer)
	submit = group user/Amit Talwar (intelliriffer)
[access "refs/heads/*"]
	read = group user/Amit Talwar (intelliriffer)
	create = group user/Amit Talwar (intelliriffer)
	push = +force group user/Amit Talwar (intelliriffer)
	pushMerge = group user/Amit Talwar (intelliriffer)
[access "refs/tags/*"]
	push = +force group user/Amit Talwar (intelliriffer)
	pushTag = group user/Amit Talwar (intelliriffer)
	pushSignedTag = group user/Amit Talwar (intelliriffer)
